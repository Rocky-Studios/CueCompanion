@page "/"
@using CueCompanion.Client.Components
@rendermode InteractiveAuto
<MudContainer MaxWidth="MaxWidth.ExtraLarge">
    <MudText Typo="Typo.h1">Dashboard</MudText>
    <MudDivider/>
    <Login></Login>
</MudContainer>
@code
{
    protected override async Task OnInitializedAsync()
