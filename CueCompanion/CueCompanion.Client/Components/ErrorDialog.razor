<MudDialog>
    <TitleContent>
        Dialog Title
    </TitleContent>
    <DialogContent>
        @ContentText
    </DialogContent>
    <DialogActions>
        <MudButton Color="Color.Primary" OnClick="Submit">OK</MudButton>
    </DialogActions>
</MudDialog>

@code {
    [CascadingParameter] private IMudDialogInstance MudDialog { get; set; }

    [Parameter] public string ContentText { get; set; }

    private void Submit()
    {
        MudDialog.Close(DialogResult.Ok(true));
    }

}